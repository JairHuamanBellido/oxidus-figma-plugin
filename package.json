{
  "name": "oxidus-color-generator",
  "version": "1.0.0",
  "description": "Generate palette color based on Shadcn",
  "author": {
    "name": "Jair Huaman Bellido",
    "url": "https://github.com/JairHuamanBellido/oxidus-figma-plugin"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run build:main",
    "build:main": "esbuild lib/code.ts --bundle --outfile=dist/code.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@preact/signals": "^1.3.0",
    "clsx": "^2.1.1",
    "color": "^4.2.3",
    "lucide-react": "^0.427.0",
    "preact": "^10.16.0",
    "tailwind-merge": "^2.4.0",
    "zustand": "^4.5.4"
  },
  "overrides": {
    "zustand": {
      "react": "npm:preact"
    }
  },
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "*",
    "@figma/plugin-typings": "*",
    "@preact/preset-vite": "^2.5.0",
    "@types/color": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-singlefile": "^0.13.5"
  }
}
